process.stdin.resume();
process.stdin.setEncoding('utf8');
// 自分の得意な言語で
// Let's チャレンジ！！
var lines = [];
var reader = require('readline').createInterface({
  input: process.stdin,
  output: process.stdout
});
reader.on('line', (line) => {
  lines.push(line);
});
reader.on('close', () => {
  let X=parseInt(lines[0]);
  let N=parseInt(lines[1]);
  if(N>=0&&N<N<=100){
      console.log((100-N)*X)
  }else{
      console.log("")
  }
});
